{
    "extends": "eslint-config-fluid",
    "env": {
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 9
    },
    "rules": {
        "strict": ["error", "global"],
        "no-loop-func": 0,
        "no-console": 0,
        "dot-notation": [2, { "allowKeywords": true }]
    }
}
