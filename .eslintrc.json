{
    "extends": "eslint-config-fluid",
    "env": {
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 2022
    },
    "rules": {
        "strict": ["error", "global"],
        "no-loop-func": 0,
        "no-console": 0,
        "quotes": [
            "error",
            "double",
            { "allowTemplateLiterals": true }
        ]
    }
}
