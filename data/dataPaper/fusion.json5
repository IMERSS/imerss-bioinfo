{
    datasets: {
        PMLS: {
            name: "Pacific Marine Life Survey Dive Records (1968-2018)",
            input: "%bagatelle/data/Galiano PMLS/PMLS_records_Galiano_2020_08_04.csv",
            map: "%bagatelle/data/Galiano PMLS/PMLS-map.json",
            outMap: "%bagatelle/data/Galiano PMLS/PMLS-out-map.json",
            colour: "#931362"
        },
        iNat: {
            name: "Galiano iNaturalist Marine Observations (2005-2020)",
            input: "%bagatelle/data/iNaturalist/iNat_Catalogue_Marine_Animalia_2020_08_04.csv",
            map: "%bagatelle/data/iNaturalist/iNaturalist-obs-map.json",
            outMap: "%bagatelle/data/iNaturalist/iNaturalist-obs-out-map.json",
            colour: "#2C8C99"
        },
        RBCM: {
            name: "Royal British Columbia Museum Voucher Specimens (1893-2011)",
            input: "%bagatelle/data/RBCM/RBCM_records_Galiano_2019-10-25.csv",
            map: "%bagatelle/data/RBCM/RBCM-map.json",
            outMap: "%bagatelle/data/RBCM/RBCM-out-map.json",
            colour: "#29a329"
        },
        BCCSN: {
            name: "British Columbia Cetacean Sightings Network (2001-2020)",
            input: "%bagatelle/data/BCCSN/BC_Cetacean_Sightings_Network_galiano_biodiversity_catalogue_2020_07_15.csv",
            map: "%bagatelle/data/BCCSN/BCCSN-map.json",
            outMap: "%bagatelle/data/BCCSN/BCCSN-out-map.json",
            colour: "#55a329"
        },
        CMN: {
            name: "Canadian Museum of Nature via VertNet (1982)",
            input: "%bagatelle/data/CMN/Vert_Net_CMN_Marine_Vertebrate_Collections_2019-04-28.csv",
            map: "%bagatelle/data/CMN/CMN-map.json",
            outMap: "%bagatelle/data/CMN/CMN-out-map.json",
            colour: "#77a329"
        }
    },
    filters: {
        Plantae: {
            field: "kingdom", // Recall that these are before outMap is applied
            equals: "Plantae",
            exclude: true
        },
        Chromista: {
            field: "kingdom", // Recall that these are before outMap is applied
            equals: "Chromista",
            exclude: true
        },
        Eupulmonata: {
            field: "superorder", // Land snails and slugs
            equals: "Eupulmonata",
            exclude: true
        },
        Hygrophila: {
            field: "superorder", // Freshwater snails
            equals: "Hygrophila",
            exclude: true
        },
        Collembola: {
            field: "subclass", // Springtails
            equals: "Collembola",
            exclude: true
        },
        Diplopoda: {
            field: "class", // Springtails
            equals: "Diplopoda",
            exclude: true
        },
        Branchiopoda: {
            field: "class", // Two freshwater exports from RBCM
            equals: "Branchiopoda",
            exclude: true
        }
    },
    summarise: true,
    output: "%bagatelle/data/dataPaper/reintegrated.csv",
    combinedOutMap: "%bagatelle/data/dataPaper/combinedOutMap.json"
}
