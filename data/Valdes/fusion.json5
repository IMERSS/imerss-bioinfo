{
    datasets: {
        PMLS: {
            name: "Pacific Marine Life Survey Valdes Dive Records (1965-2015)",
            input: "%bagatelle/data/Valdes/Valdes marine data (dives included).csv",
            map: "%bagatelle/data/Valdes/Valdes-PMLS-map.json",
            outMap: "%bagatelle/data/Valdes/Valdes-PMLS-out-map.json",
            colour: "#931362"
        },
        iNat: {
            name: "Valdes iNaturalist Observations (2013-2021)",
            input: "%bagatelle/data/Valdes/iNaturalist_Catalogue_2021_10_12.csv",
            map: "%bagatelle/data/iNaturalist/iNaturalist-obs-map-new.json",
            outMap: "%bagatelle/data/iNaturalist/iNaturalist-obs-out-map.json",
            colour: "#2C8C99"
        },
        RBCM: {
            name: "Royal British Columbia Museum Valdes Island Voucher Specimens (1920-2018)",
            input: "%bagatelle/data/Valdes/Valdes Island - RBCM - 2021_10_15.csv",
            map: "%bagatelle/data/Valdes/Valdes-synth-map.json",
            outMap: "%bagatelle/data/Valdes/Valdes-synth-out-map.json",
            colour: "#29a329"
        },
        UBC: {
            name: "University of British Columbia Valdes Island Collections (1905-2018)",
            input: "%bagatelle/data/Valdes/Valdes Island - UBC - 2021_10_15.csv",
            map: "%bagatelle/data/Valdes/Valdes-synth-map.json",
            outMap: "%bagatelle/data/Valdes/Valdes-synth-out-map.json",
            colour: "#55a329"
        },
        VIC: {
            name: "Valdes Island Conservancy Datasets (2014-2019)",
            input: "%bagatelle/data/Valdes/Valdes Island - VIC - 2021_10_15.csv",
            map: "%bagatelle/data/Valdes/Valdes-synth-map.json",
            outMap: "%bagatelle/data/Valdes/Valdes-synth-out-map.json",
            colour: "#a7a329"
        },
        ASIMON: {
            name: "Andrew Simon Valdes Island Species Inventory (2017-2018)",
            input: "%bagatelle/data/Valdes/Valdes Island - ASIMON - 2021_10_15.csv",
            map: "%bagatelle/data/Valdes/Valdes-synth-map.json",
            outMap: "%bagatelle/data/Valdes/Valdes-synth-out-map.json",
            colour: "#f7a329"
        }
    },
    patches: {
        Deprivatise: {
            processor: "hortis.deprivatise"
        },
        RoundCoordinates: {
            type: "hortis.pipe",
            processor: "hortis.roundCoordinates",
            places: 6
        },
