{
    datasets: {
        PMLS: {
            name: "Pacific Marine Life Survey Valdes Dive Records (1965-2015)",
            input: "%bagatelle/data/Valdes/Valdes marine data (dives included).csv",
            map: "%bagatelle/data/Valdes/Valdes-PMLS-map.json",
            outMap: "%bagatelle/data/Valdes/Valdes-PMLS-out-map.json",
            colour: "#931362"
        },
        iNat: {
            name: "Valdes iNaturalist Observations (2013-2021)",
            input: "%bagatelle/data/Valdes/iNaturalist_Catalogue_2021_10_12.csv",
            map: "%bagatelle/data/iNaturalist/iNaturalist-obs-map-new.json",
            outMap: "%bagatelle/data/iNaturalist/iNaturalist-obs-out-map.json",
            colour: "#2C8C99"
        },
        Synth: {
            name: "Valdes Island Synthesized Datasets (1972-2018)",
            input: "%bagatelle/data/Valdes/Valdes Island - Synthesized - 2021_10_13.csv",
            map: "%bagatelle/data/Valdes/Valdes-synth-map.json",
            outMap: "%bagatelle/data/Valdes/Valdes-synth-out-map.json",
            colour: "#29a329"
        },
    },
    patches: {
        Deprivatise: {
            processor: "hortis.deprivatise"
        },
        RoundCoordinates: {
            type: "hortis.pipe",
            processor: "hortis.roundCoordinates",
            places: 6
        },
/*        ValdesRegion: {
            type: "hortis.pipe.JSONInput",
            input: "%bagatelle/data/Valdes/valdes-island.json",
            processor: "hortis.processRegionFilter",
            logRejection: true
        }
*/  },
    summarise: true,
    applySwaps: true, // TODO: Naturally, should be an actual pipeline element
    output: "%bagatelle/data/Valdes/reintegrated.csv",
    combinedOutMap: "%bagatelle/data/Valdes/combinedOutMap.json",
    filters: {
    }
}
