            type: "hortis.pipe.JSONInput",
            input: "%bagatelle/data/Valdes/valdes-island.json",
            processor: "hortis.processRegionFilter",
            logRejection: true
        }
*/  },
    summarise: true,
    applySwaps: true, // TODO: Naturally, should be an actual pipeline element
    output: "%bagatelle/data/Valdes/reintegrated.csv",
    combinedOutMap: "%bagatelle/data/Valdes/combinedOutMap.json",
    filters: {
    }
}
