{
    datasets: {
        PMLS: {
            name: "Pacific Marine Life Surveys (1965-2015)",
            input: "%bagatelle/data/Valdes/Valdes marine data (dives included).csv",
            map: "%bagatelle/data/Valdes/Valdes-PMLS-map.json",
            outMap: "%bagatelle/data/Valdes/Valdes-PMLS-out-map.json",
            colour: "#931362"
        },
        iNat: {
            name: "iNaturalist (2013-2021)",
            input: "%bagatelle/data/Valdes/iNaturalist_Catalogue_2021_10_12.csv",
            map: "%bagatelle/data/iNaturalist/iNaturalist-obs-map-new.json",
            outMap: "%bagatelle/data/iNaturalist/iNaturalist-obs-out-map.json",
            colour: "#2C8C99"
        },
        RBCM: {
            name: "Royal BC Museum (1920-2018)",
            input: "%bagatelle/data/Valdes/Valdes Island - RBCM - 2022_04_13.csv",
            map: "%bagatelle/data/Valdes/Valdes-synth-map.json",
            outMap: "%bagatelle/data/Valdes/Valdes-synth-out-map.json",
            colour: "#29a329"
        },
        UBC: {
            name: "UBC Beaty Biodiversity Museum (1905-2018)",
            input: "%bagatelle/data/Valdes/Valdes Island - UBC - 2022_04_13.csv",
            map: "%bagatelle/data/Valdes/Valdes-synth-map.json",
            outMap: "%bagatelle/data/Valdes/Valdes-synth-out-map.json",
            colour: "#55a329"
        },
        VIC: {
            name: "Valdes Island Conservancy Surveys (2014-2019)",
            input: "%bagatelle/data/Valdes/Valdes Island - VIC - 2022_04_13.csv",
            map: "%bagatelle/data/Valdes/Valdes-synth-map.json",
            outMap: "%bagatelle/data/Valdes/Valdes-synth-out-map.json",
            colour: "#a7a329"
        }
    },
    patches: {
        Deprivatise: {
            processor: "hortis.deprivatise"
        },
        RoundCoordinates: {
            type: "hortis.pipe",
            processor: "hortis.roundCoordinates",
            places: 6
        },
/*        ValdesRegion: {
            type: "hortis.pipe.JSONInput",
            input: "%bagatelle/data/Valdes/valdes-island.json",
            processor: "hortis.processRegionFilter",
            logRejection: true
        }
*/  },
    summaryPatches: {
        AssignHulqNames: {
            type: "hortis.pipe.CSVInput",
            input: "%bagatelle/data/Xetthecum/Culturally_significant_species_synthesized_Thom_2011_Turner_2014_&_Luschim_2021.csv",
            map: "%bagatelle/data/Xetthecum/Culturally_significant-map.json",
            processor: "hortis.xetthecum.assignHulqNames"
        }
    },
    summarise: true,
    applySwaps: true, // TODO: Naturally, should be an actual pipeline element
    output: "%bagatelle/data/Valdes/reintegrated.csv",
    combinedOutMap: "%bagatelle/data/Valdes/combinedOutMap.json",
    filters: {
    }
}
