{
    datasets: {
        dataPaper: {
            name: "Galiano Data Paper Part I - Marine Observations (1968-2021)",
            input: "%bagatelle/data/dataPaper-I/reintegrated-obs.csv",
            map: "%bagatelle/data/dataPaper-I/combinedOutMap-obs.json",
            templateMap: false,
            outMap: "%bagatelle/data/dataPaper-I/combinedOutMap-obs.json",
            colour: "#931362"
        },
        iNat: {
            name: "Xetthecum iNaturalist Observations (2005-2022)",
            input: "%bagatelle/data/Xetthecum/iNaturalist_Catalogue_2022_02_28.csv",
            map: "%bagatelle/data/iNaturalist/iNaturalist-obs-map-new.json",
            outMap: "%bagatelle/data/iNaturalist/iNaturalist-obs-out-map.json",
            colour: "#2C8C99"
        },
        Hunterston: {
            name: "Hunterston Farm Stewardship Report: A Review of the 2010 BioBlitz (2010)",
            input: "%bagatelle/data/Xetthecum/Hunterston_Farms_Bioblitz_2010_coordinates_corrected_mismatches_reconciled.csv",
            map: "%bagatelle/data/Xetthecum/hunterston-map.json",
            outMap: "%bagatelle/data/Xetthecum/hunterston-out-map.json",
            colour: "#55a329"
        },
    },
    patches: {
        Deduplicate: {
            processor: "hortis.deduplicateById",
            idField: "observationId"
        },
        Deprivatise: {
            processor: "hortis.deprivatise"
        },
/*        PhaseICoordinates: {
            type: "hortis.pipe.CSVInput",
            input: "%bagatelle/data/dataPaper-I/Corrected_georeferencing_marine_animal_occurrence_records_corrected_2021-01-13-plus-round-8.csv",
            map: "%bagatelle/data/dataPaper-I/correctedGeoreferencing-map.json",
            processor: "hortis.processCoordinatePatch"
        },*/
        RoundCoordinates: {
            type: "hortis.pipe",
            processor: "hortis.roundCoordinates",
            places: 6
        },
        XetthecumRegion: {
            type: "hortis.pipe.JSONInput",
            input: "%bagatelle/data/Xetthecum/Xetthecum_Project_Boundary_3.1.geojson",
            processor: "hortis.processRegionFilter",
            logRejection: false
        },
        AssignXetthecumFeature: {
            type: "hortis.pipe.JSONInput",
            input: "%bagatelle/data/Xetthecum/flatFeatures.json",
            processor: "hortis.processAssignFeature"
        },
    },
    summaryPatches: {
        AssignHulqNames: {
            type: "hortis.pipe.CSVInput",
            input: "%bagatelle/data/Xetthecum/Culturally_significant_species_synthesized_Thom_2011_&_Turner_2014.csv",
            map: "%bagatelle/data/Xetthecum/Culturally_significant-map.json",
            processor: "hortis.xetthecum.assignHulqNames"
        },
        AssignHulqMedia: {
            type: "hortis.pipe.CSVInput",
            input: "%bagatelle/data/Xetthecum/Species_media.csv",
            map: "%bagatelle/data/Xetthecum/Species_media-map.json",
            processor: "hortis.xetthecum.assignHulqMedia"
        },
    },
    summaryOutMapExtraColumns: {
        // TODO: In case we want an obs file with these fields
    },
    summarise: true,
    applySwaps: true, // TODO: Naturally, should be an actual pipeline element
    output: "%bagatelle/data/Xetthecum/reintegrated.csv",
    combinedOutMap: "%bagatelle/data/Xetthecum/combinedOutMap.json",
    filters: {
    }
}
